{
  "version": "1.0.0",
  "name": "Libretto Universitario",
  "description": "Semplice applicazione per tenere traccia dei tuoi risultati universitari",
  "launch_path": "/LibrettoUniversitario/index.html",
  "appcache_path": "/LibrettoUniversitario/manifest.appcache",
  "fullscreen": "true",
  "icons": {
    "48": "/LibrettoUniversitario/icon-48.png",
    "128": "/LibrettoUniversitario/icon-128.png"
  },
  "developer": {
    "name": "Domenico Luciani",
    "url": "http://dlion.it"
  },
  "default_locale": "it",
  "installs_allowed_from": [
    "*"
  ],
  "locales": {
      "it": {
          "name": "Libretto Universitario",
          "description": "Semplice applicazione per tenere traccia dei tuoi risultati universitari"
      }
  },
  "activities": {
      "viewGithub": {
          "filters": {
              "type": "url",
              "url": {
                  "required": "true",
                  "regexp": "/^http?:/"
              },
              "href": "http://github.com/DLion"
          }
      },
      "viewTwitter": {
          "filters": {
              "type": "url",
              "url": {
                  "required": "true",
                  "regexp": "/^http?:/"
              },
              "href": "http://twitter.com/dlion92"
          }
      },
      "viewSite": {
          "filters": {
              "type": "url",
              "url": {
                  "required": "true",
                  "regexp": "/http?:/"
              },
              "href": "http://dlion.it"
          }
      }
  }
}
